function printSingle(n)
{
  var str = "";
  for(var i = 0 ; i < n ;i ++)
  {     
      str = str + "*";
  }
  console.log(str);
    
}

function singleRow(n)
{
  var star = "";
  
  star = star + "*";

  for(var i=0; i<n-2; i++)
    {
      star = star + " ";
    }
  star = star + "*";
    console.log(star);
}

function wholeThing(n)
{
  printSingle(n);
  for(var i = 0 ; i < n ; i++){
    singleRow(n);
  }
  printSingle(n);
}

wholeThing(5);
